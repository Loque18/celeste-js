{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "import/no-unresolved": [2, { "commonjs": true, "amd": true }],
        "arrow-body-style": "off"
    }
}
